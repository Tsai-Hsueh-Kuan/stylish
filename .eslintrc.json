{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": 0,
        "no-empty": 0,
        "no-irregular-whitespace": 0,
        "quotes": 1,
        "camelcase": "off"
    }
    
}
